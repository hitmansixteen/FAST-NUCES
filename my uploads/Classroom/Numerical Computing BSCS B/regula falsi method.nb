 RowBox[{
  RowBox[{"regulaFalsiMethodMethod", "[", 
   RowBox[{"f_", ",", "val1_", ",", "val2_", ",", "tol_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", ",", "b", ",", "c", ",", "fc"}], "}"}], ",", 
